<div class="container px-5 px-sm-0">
 <div class="row">
  <%= render'books/list', user:@user, book:@book %>
  <% @books.each do |book| %>
   <div>
    <%= attachment_image_tag book.user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %>
    <%= link_to book_path(book.id) do %>
    <%= book.title %>
    <% end %>
    <p><%= book.body %></p>
   </div>
  <% end %>
 </div>
</div>